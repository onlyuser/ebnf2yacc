(grammar
    (definitions
        (def_proto_block
            "\n    #include <stdio.h>\n    int yylex(void);\n    void yyerror(char *);\n"
        )
        (definition
            token
            (def_symbols
                (def_symbol
                    INTEGER
                )
            )
        )
    )
    (rules
        (rule
            program
            (rule_alts
                (rule_alt
                    (rule_terms
                        program
                        expr
                        '\n'
                    )
                    (rule_action_block
                        " printf(\"%d\\n\", $2); "
                    )
                )
                (rule_alt
                    (NULL)

                )
            )
        )
        (rule
            expr
            (rule_alts
                (rule_alt
                    (rule_terms
                        INTEGER
                    )
                    (NULL)

                )
                (rule_alt
                    (rule_terms
                        expr
                        '+'
                        expr
                    )
                    (rule_action_block
                        " $$ = $1 + $3; "
                    )
                )
                (rule_alt
                    (rule_terms
                        expr
                        '-'
                        expr
                    )
                    (rule_action_block
                        " $$ = $1 - $3; "
                    )
                )
            )
        )
    )
    (code
        "\n\nvoid yyerror(char *s) {\n    fprintf(stderr, \"%s\\n\", s);\n}\n\nint main(void) {\n    yyparse();\n    return 0;\n}\n"
    )
)
